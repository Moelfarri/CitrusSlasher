; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Line2D",
"class": "Trail2D",
"language": "GDScript",
"path": "res://Addons/trail_2d.gd"
} ]
_global_script_class_icons={
"Trail2D": "res://Addons/icon_trail_2d.svg"
}

[application]

config/name="Citrus Slasher"
run/main_scene="res://UI_script/Main Menu.tscn"
run/low_processor_mode_sleep_usec=10000
config/icon="res://icon.png"

[autoload]

Global="*res://Source_script/Global.gd"
Combo="*res://Effects_script/Combo.gd"

[display]

window/size/width=512
window/size/height=288
window/size/test_width=512
window/size/test_height=288
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

environment/default_environment="res://default_env.tres"
